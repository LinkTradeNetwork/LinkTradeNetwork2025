{
  "navigationFallback": {
    "rewrite": "/front.html",
    "exclude": [
      "/assets/*",
      "/*.{css,js,png,jpg,jpeg,gif,svg,ico,webp}"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/front.html",
      "statusCode": 200
    }
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript",
    ".css": "text/css",
    ".html": "text/html"
  }
}
